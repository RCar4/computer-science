#011
def numbers():
    num11 = float(input("enter a number above 100: "))
    num22 = float(input("enter a number below 10: "))
    answer = num11/num22
    print(answer)
    
    if num22 > 10:
        print("BELOW 10")
        return numbers()
    
    if num11 < 100:
        print("ABOVE 100")
        return numbers()
        

numbers()